#-*- mode: snakemake -*-
"""
Snakemake rules for quality control of small-rna.
"""

QC_INTERIM = join(INTERIM_DIR, 'smallrna', 'qc')
     
include:
    'qc/fastq.rules'


rule qc_all:
    input:
        expand(rules.qc_fastq_fastqc.output, sample=SAMPLES),
        expand(rules.qc_fastq_fastqscreen.output, sample=SAMPLES),
        rules.qc_fastq_mirtrace.output
        
            
