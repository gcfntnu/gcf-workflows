workflow: singlecell
quant:
  method: cellranger
  doublets:
    method: demuxafy
  demultiplex:
    with_reference: false
    method: vireo
    donor_dir: null
    n: 4
    sample_keys: ['donor_ids', 'donor_files']
  aggregate:
    skip: false
    method: cellranger
    norm: 'mapped'
    groupby: all_samples
  cellranger:
    mkgtf:
      gene_biotype:
        - protein_coding
        - lincRNA
    reanalyze:
      skip: true
    
